{
  "name": "emedia/generators",
  "description": "Scaffolding Generator Package for Laravel 5",
  "type": "library",
  "repositories": [
    {
      "type":"git",
      "url":"https://github.com/emedia1/quickdata-laravel.git"
    },
    {
      "type": "git",
      "url": "https://github.com/emedia1/laravel-helpers.git"
    },
    {
      "type": "git",
      "url": "https://github.com/emedia1/php-helpers.git"
    }
  ],
  "require": {
    "php": ">=5.6",
    "illuminate/console": "^5.7",
    "emedia/quick-data-laravel": "1.0"
  },
  "license": "proprietary",
  "authors": [
    {
      "name": "Shane Perera",
      "email": "shane@elegantmedia.com.au"
    }
  ],
  "autoload": {
    "psr-4": {
      "EMedia\\Generators\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "EMedia\\Generators\\GeneratorServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
